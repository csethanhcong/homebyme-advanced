<script type="text/javascript">
	$(function() {
		/*Tooltip init*/
		$('[data-toggle="tooltip"]').tooltip();
		/*Hide Tools*/
		$('#tool').hide();

		/*Display the left panel with correct responding*/
		$('#room-selector').hide();
