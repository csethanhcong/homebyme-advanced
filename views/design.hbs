		// global var-used for bill processing
		var count = 0;

		$('#appartment-selector ul li a').hover(
			function() {
			/* Stuff to do when the mouse enters the element */
			var liIndex = $(this).parent().index();
			switch (liIndex) {
				case 0: 
					$('.working-space').css({
						'background-image' : 'url("./images/Appartment/Real/Cidrap.png")',
						'background-position' : 'center'
					});
					break;
				case 2: 
					$('.working-space').css({
						'background-image' : 'url("./images/Appartment/Real/Idico.png")',
						'background-position' : 'center'
					});
					break;
				case 4: 
					$('.working-space').css({
						'background-image' : 'url("./images/Appartment/Real/Oaks.png")',
						'background-position' : 'center'
					});
